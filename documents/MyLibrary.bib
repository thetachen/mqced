@article{puthumpally-joseph_theoretical_2015,
	title = {Theoretical analysis of dipole-induced electromagnetic transparency},
	volume = {91},
	url = {https://link.aps.org/doi/10.1103/PhysRevA.91.043835},
	doi = {10.1103/PhysRevA.91.043835},
	abstract = {We present a detailed, realistic analysis of the implementation of a proposal for dipole-induced electromagnetic transparency (DIET) [R. Puthumpally-Joseph, M. Sukharev, O. Atabek, and E. Charron, Phys. Rev. Lett. 113, 163603 (2014)] using an ensemble of cold atoms at high density. Using both direct numerical simulations and simple analytical models, we show how, in a realistic N-level quantum system, narrow transparency windows can appear at large densities. The existence of such windows is attributed to quantum interference effects in overlapping resonances. Our analysis is applied to the D1 transition of Rb atoms, and we show that, at high densities, Rb can behave like a simple three-level emitter exhibiting all the properties of DIET. Some interesting effects such as slow light are also presented, and their limits in the context of DIET are discussed},
	number = {4},
	urldate = {2018-01-26},
