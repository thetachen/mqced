@article{hwang_analysis_2004,
	title = {An analysis of electronic dephasing in the spin-boson model.},
	volume = {120},
	issn = {0021-9606},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/15268171},
	doi = {10.1063/1.1742979},
	abstract = {In order to develop a more complete understanding of the limitations of mixed quantum-classical simulation methods, the origins of electronic dephasing are analyzed in a simple model of the condensed phase, namely, the spin-boson model with an ohmic spectral density. We focus on the decay of the thermally averaged nuclear overlap/phase function (NOPF). Considering the strong coupling/high temperature limit, a relationship is obtained at short time between the rate of electronic coherence loss and the electronic dephasing rate characteristic of a classical bath. Using this relationship, we clarify the origin of the decay of the NOPF. In the same limit, we also reproduce an earlier relationship between the electronic decoherence time and a solvation relaxation time. Finally, we point out that, for the spin-boson model, the exact quantum mechanical description of electronic dephasing is reproduced by mixed quantum/classical methods if a Gaussian distribution of quantum fluctuations around each classical phase space point is introduced. That spatial distribution of quantum fluctuations is functionally the same as that appearing in the Feynman-Kleinert variational local harmonic approximation, and also that implemented in existing classical trajectory-based estimates of coherence dissipation times.},
	number = {24},
	journal = {J. Chem. Phys.},
	author = {Hwang, Hyonseok and Rossky, Peter J},
	month = jun,
	year = {2004},
	pmid = {15268171},
	pages = {11380--5}
}

@article{ishizaki_adequacy_2009,
	title = {On the adequacy of the {Redfield} equation and related approaches to the study of quantum dynamics in electronic energy transfer.},
	volume = {130},
	issn = {1089-7690},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/19548714},
	doi = {10.1063/1.3155214},
	abstract = {The observation of long-lived electronic coherence in photosynthetic excitation energy transfer (EET) by Engel et al. [Nature (London) 446, 782 (2007)] raises questions about the role of the protein environment in protecting this coherence and the significance of the quantum coherence in light harvesting efficiency. In this paper we explore the applicability of the Redfield equation in its full form, in the secular approximation and with neglect of the imaginary part of the relaxation terms for the study of these phenomena. We find that none of the methods can give a reliable picture of the role of the environment in photosynthetic EET. In particular the popular secular approximation (or the corresponding Lindblad equation) produces anomalous behavior in the incoherent transfer region leading to overestimation of the contribution of environment-assisted transfer. The full Redfield expression on the other hand produces environment-independent dynamics in the large reorganization energy region. A companion paper presents an improved approach, which corrects these deficiencies [A. Ishizaki and G. R. Fleming, J. Chem. Phys. 130, 234111 (2009)].},
	number = {23},
	journal = {J. Chem. Phys.},
	author = {Ishizaki, Akihito and Fleming, Graham R},
	month = jun,
	year = {2009},
	pmid = {19548714},
	keywords = {Quantum Theory, Electrons, Energy transfer, Models, Computer Simulation, Chemical, Ishizaki/Fleming},
	pages = {234110}
}

@article{ishizaki_theoretical_2009,
	title = {Theoretical examination of quantum coherence in a photosynthetic system at physiological temperature.},
	volume = {106},
	issn = {1091-6490},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2762676&tool=pmcentrez&rendertype=abstract},
	doi = {10.1073/pnas.0908989106},
	abstract = {The observation of long-lived electronic coherence in a photosynthetic pigment-protein complex, the Fenna-Matthews-Olson (FMO) complex, is suggestive that quantum coherence might play a significant role in achieving the remarkable efficiency of photosynthetic electronic energy transfer (EET), although the data were acquired at cryogenic temperature [Engel GS, et al. (2007) Evidence for wavelike energy transfer through quantum coherence in photosynthetic systems. Nature 446:782-786]. In this paper, the spatial and temporal dynamics of EET through the FMO complex at physiological temperature are investigated theoretically. The numerical results reveal that quantum wave-like motion persists for several hundred femtoseconds even at physiological temperature, and suggest that the FMO complex may work as a rectifier for unidirectional energy flow from the peripheral light-harvesting antenna to the reaction center complex by taking advantage of quantum coherence and the energy landscape of pigments tuned by the protein scaffold. A potential role of quantum coherence is to overcome local energetic traps and aid efficient trapping of electronic energy by the pigments facing the reaction center complex.},
	number = {41},
	journal = {Proc. Natl. Acad. Sci. U. S. A.},
	author = {Ishizaki, Akihito and Fleming, Graham R},
	month = oct,
	year = {2009},
	pmid = {19815512},
	keywords = {Electronics, Quantum Theory, Chlorobi, Photosynthesis, Energy transfer, Models, Ishizaki/Fleming, Biological, Biological Evolution, Chlorobi: physiology, Energy Transfer: physiology, Photosynthesis: physiology, Rhodobacter sphaeroides, Rhodobacter sphaeroides: physiology, Temperature},
	pages = {17255--60}
}

