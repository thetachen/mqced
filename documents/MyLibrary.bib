@article{jasper_improved_2003,
	title = {Improved treatment of momentum at classically forbidden electronic transitions in trajectory surface hopping calculations},
	volume = {369},
	issn = {00092614},
	url = {http://www.sciencedirect.com/science/article/pii/S0009261402019905},
