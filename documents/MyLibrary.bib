	pages = {174110},
	file = {Full Text PDF:/home/theta/.zotero/zotero/s81xtrsi.default/zotero/storage/SR5BS8PU/Jensen et al. - 2005 - Theory and method for calculating resonance Raman .pdf:application/pdf;Snapshot:/home/theta/.zotero/zotero/s81xtrsi.default/zotero/storage/WXTVWKID/1.html:text/html}
