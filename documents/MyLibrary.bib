@article{dash_nonequilibrium_2011,
	title = {Nonequilibrium inelastic electronic transport: {Polarization} effects and vertex corrections to the self-consistent {Born} approximation},
	volume = {84},
	issn = {10980121},
	url = {http://link.aps.org/doi/10.1103/PhysRevB.84.085433},
	doi = {10.1103/PhysRevB.84.085433},
	abstract = {We study the effect of electron-vibron interactions on the inelastic transport properties of single-molecule nanojunctions. We use the nonequilibrium Green's functions technique and a model Hamiltonian to calculate the effects of second-order diagrams [double-exchange (DX) and dressed-phonon (DPH) diagrams] on the electron-vibration interaction and consider their effects across the full range of parameter space. The DX diagram, corresponding to a vertex correction, introduces an effective dynamical renormalization of the electron-vibron coupling in both the purely inelastic and the inelastic-resonant features of the inelastic electron tunneling spectrum. The purely inelastic features correspond to an applied bias around the energy of a vibron, while the inelastic-resonant features correspond to peaks (resonance) in the conductance. The DPH diagram affects only the inelastic resonant features. We also discuss the circumstances in which the second-order diagrams may be approximated in the study of more complex model systems.},
	number = {8},
	journal = {Phys. Rev. B},
	author = {Dash, L. K. and Ness, H. and Godby, R. W.},
	month = aug,
	year = {2011},
	keywords = {Dash, IETS, SCBA},
	pages = {085433}
}

